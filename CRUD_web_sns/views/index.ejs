<div class="content">
    <h1>마이페이지</h1>
    
    <img id = "profile_img" src="<%= UserData.profile_img %>" />

    <button type = "button">프로필 편집</button>




    <!-- user 테이블에 있는 정보 -->
    <form name="form_user">
        <label>ID</label><br/>
        <input type="text" name="user_id" value="<%= UserData.user_id %>"><br/>

        <label>PW</label><br/>
        <input type="text" name="user_pw" value="<%= UserData.user_pw %>"><br/>

        <label>NAME</label><br/>
        <input type="text" name="user_name" value="<%= UserData.user_name %>"><br/>

        <label>BIRTH</label><br/>
        <input type="text" name="user_birth" value="<%= UserData.user_birth %>"><br/>
        
        <button type="button" onclick="editProfile();">수정하기</button>
    </form>

    <!-- post 테이블에 있 -->
    <form name="form_post">
        <% for(let i = 0; i < PostData.length; i++) { %>
            <img src = "<%= PostData[i].img_src %>">
        <% } %>
    </form>

</div>


